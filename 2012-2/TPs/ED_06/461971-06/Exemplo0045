#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00A03A58 .scope module, "Exemplo0045" "Exemplo0045" 2 22;
 .timescale 0 0;
v00A0ED58_0 .net "clk", 0 0, v00A0ED00_0; 1 drivers
v00A048F0_0 .net "p1", 0 0, v00A030C8_0; 1 drivers
S_00A0EC78 .scope module, "CLK1" "clock" 2 24, 3 5, S_00A03A58;
 .timescale 0 0;
v00A0ED00_0 .var "clk", 0 0;
S_00A02FE8 .scope module, "pulse1" "pulse" 2 26, 2 6, S_00A03A58;
 .timescale 0 0;
v00A03070_0 .alias "clock", 0 0, v00A0ED58_0;
v00A030C8_0 .var "signal", 0 0;
E_00A09350 .event posedge, v00A03070_0;
    .scope S_00A0EC78;
T_0 ;
    %set/v v00A0ED00_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_00A0EC78;
T_1 ;
    %delay 12, 0;
    %load/v 8, v00A0ED00_0, 1;
    %inv 8, 1;
    %set/v v00A0ED00_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00A02FE8;
T_2 ;
    %wait E_00A09350;
    %load/v 8, v00A030C8_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_2.0, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v00A030C8_0, 0, 0;
    %jmp T_2.1;
T_2.0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v00A030C8_0, 0, 1;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_00A03A58;
T_3 ;
    %vpi_call 2 28 "$dumpfile", "Exemplo0045.vcd";
    %vpi_call 2 29 "$dumpvars";
    %delay 1200, 0;
    %vpi_call 2 30 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Exemplo0045.v";
    "./clock.v";
