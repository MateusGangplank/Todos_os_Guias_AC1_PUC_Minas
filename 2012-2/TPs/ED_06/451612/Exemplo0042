#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_007C4A90 .scope module, "Exemplo0042" "Exemplo0042" 2 45;
 .timescale 0 0;
v007CACF0_0 .net "c", 0 0, v007CDA28_0; 1 drivers
v007CAD48_0 .var "p", 0 0;
v007CADA0_0 .net "p1", 0 0, v007CD9D0_0; 1 drivers
v007CADF8_0 .net "t1", 0 0, v007C2D40_0; 1 drivers
S_006CF810 .scope module, "clk" "clock" 2 47, 2 11, S_007C4A90;
 .timescale 0 0;
v007CDA28_0 .var "clk", 0 0;
S_007C2D98 .scope module, "pulse1" "pulse" 2 50, 2 23, S_007C4A90;
 .timescale 0 0;
v007CD978_0 .alias "clock", 0 0, v007CACF0_0;
v007CD9D0_0 .var "signal", 0 0;
E_007CCFE0 .event edge, v007C37E8_0;
S_007C3708 .scope module, "trigger1" "trigger" 2 51, 2 35, S_007C4A90;
 .timescale 0 0;
L_007CE3C0 .functor AND 1, v007CDA28_0, v007CAD48_0, C4<1>, C4<1>;
v007C3790_0 .net *"_s1", 0 0, L_007CE3C0; 1 drivers
v007C37E8_0 .alias "clock", 0 0, v007CACF0_0;
v007C2CE8_0 .net "on", 0 0, v007CAD48_0; 1 drivers
v007C2D40_0 .var "signal", 0 0;
E_007CD000 .event posedge, L_007CE3C0;
    .scope S_006CF810;
T_0 ;
    %set/v v007CDA28_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_006CF810;
T_1 ;
    %delay 12, 0;
    %load/v 8, v007CDA28_0, 1;
    %inv 8, 1;
    %set/v v007CDA28_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_007C2D98;
T_2 ;
    %wait E_007CCFE0;
    %set/v v007CD9D0_0, 1, 1;
    %delay 3, 0;
    %set/v v007CD9D0_0, 0, 1;
    %delay 3, 0;
    %set/v v007CD9D0_0, 1, 1;
    %delay 3, 0;
    %set/v v007CD9D0_0, 0, 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_007C3708;
T_3 ;
    %wait E_007CD000;
    %delay 60, 0;
    %set/v v007C2D40_0, 1, 1;
    %delay 60, 0;
    %set/v v007C2D40_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_007C4A90;
T_4 ;
    %set/v v007CAD48_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_007C4A90;
T_5 ;
    %vpi_call 2 56 "$dumpfile", "Exemplo0042.vcd";
    %vpi_call 2 57 "$dumpvars", 2'sb01, v007CACF0_0, v007CADA0_0, v007CAD48_0, v007CADF8_0;
    %delay 60, 0;
    %set/v v007CAD48_0, 1, 1;
    %delay 120, 0;
    %set/v v007CAD48_0, 0, 1;
    %delay 180, 0;
    %set/v v007CAD48_0, 1, 1;
    %delay 240, 0;
    %set/v v007CAD48_0, 0, 1;
    %delay 300, 0;
    %set/v v007CAD48_0, 1, 1;
    %delay 360, 0;
    %set/v v007CAD48_0, 0, 1;
    %delay 376, 0;
    %vpi_call 2 64 "$finish";
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 3;
