#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0080E718 .scope module, "testxorgate" "testxorgate" 2 19;
 .timescale 0 0;
v0080FE00_0 .var "a", 0 3;
v0080FE58_0 .var "b", 0 3;
v0080FEB0_0 .net "s", 0 3, L_00802D90; 1 drivers
S_00802D08 .scope module, "XOR1" "xorgate" 2 24, 2 11, S_0080E718;
 .timescale 0 0;
L_00802D90 .functor XOR 4, v0080FE00_0, v0080FE58_0, C4<0000>, C4<0000>;
v00803A48_0 .net "p", 0 3, v0080FE00_0; 1 drivers
v00803AA0_0 .net "q", 0 3, v0080FE58_0; 1 drivers
v00803AF8_0 .alias "s", 0 3, v0080FEB0_0;
S_00802C80 .scope begin, "start" "start" 2 26, 2 26, S_0080E718;
 .timescale 0 0;
S_0080E7A0 .scope begin, "main" "main" 2 31, 2 31, S_0080E718;
 .timescale 0 0;
    .scope S_0080E718;
T_0 ;
    %fork t_1, S_00802C80;
    %jmp t_0;
    .scope S_00802C80;
t_1 ;
    %movi 8, 3, 4;
    %set/v v0080FE00_0, 8, 4;
    %movi 8, 5, 4;
    %set/v v0080FE58_0, 8, 4;
    %end;
    .scope S_0080E718;
t_0 %join;
    %end;
    .thread T_0;
    .scope S_0080E718;
T_1 ;
    %fork t_3, S_0080E7A0;
    %jmp t_2;
    .scope S_0080E7A0;
t_3 ;
    %vpi_call 2 32 "$display", "Exemplo0005 - Rodolfo Herman - 451612";
    %vpi_call 2 33 "$display", "Test xor gate";
    %vpi_call 2 34 "$display", "\012 a ^ b = s\012";
    %vpi_call 2 35 "$monitor", "%b ^ %b = %b", v0080FE00_0, v0080FE58_0, v0080FEB0_0;
    %delay 1, 0;
    %set/v v0080FE00_0, 0, 4;
    %set/v v0080FE58_0, 0, 4;
    %delay 1, 0;
    %movi 8, 2, 4;
    %set/v v0080FE00_0, 8, 4;
    %movi 8, 1, 4;
    %set/v v0080FE58_0, 8, 4;
    %delay 1, 0;
    %movi 8, 1, 4;
    %set/v v0080FE00_0, 8, 4;
    %movi 8, 3, 4;
    %set/v v0080FE58_0, 8, 4;
    %delay 1, 0;
    %movi 8, 5, 4;
    %set/v v0080FE00_0, 8, 4;
    %movi 8, 2, 4;
    %set/v v0080FE58_0, 8, 4;
    %delay 1, 0;
    %movi 8, 10, 4;
    %set/v v0080FE00_0, 8, 4;
    %movi 8, 3, 4;
    %set/v v0080FE58_0, 8, 4;
    %delay 1, 0;
    %movi 8, 9, 4;
    %set/v v0080FE00_0, 8, 4;
    %movi 8, 3, 4;
    %set/v v0080FE58_0, 8, 4;
    %end;
    .scope S_0080E718;
t_2 %join;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
