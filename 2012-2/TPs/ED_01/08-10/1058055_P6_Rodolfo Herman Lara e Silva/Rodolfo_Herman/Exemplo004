#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00612C80 .scope module, "testorgate" "testorgate" 2 18;
 .timescale 0 0;
v0061E0E0_0 .var "a", 0 0;
v0061E138_0 .var "b", 0 0;
v0061E190_0 .net "s", 0 0, L_00612D90; 1 drivers
S_00613150 .scope module, "OR1" "orgate" 2 23, 2 11, S_00612C80;
 .timescale 0 0;
L_00612D90 .functor OR 1, v0061E0E0_0, v0061E138_0, C4<0>, C4<0>;
v0061E5B8_0 .net "p", 0 0, v0061E0E0_0; 1 drivers
v006131D8_0 .net "q", 0 0, v0061E138_0; 1 drivers
v00613230_0 .alias "s", 0 0, v0061E190_0;
S_00612D08 .scope begin, "start" "start" 2 25, 2 25, S_00612C80;
 .timescale 0 0;
    .scope S_00612C80;
T_0 ;
    %fork t_1, S_00612D08;
    %jmp t_0;
    .scope S_00612D08;
t_1 ;
    %set/v v0061E0E0_0, 0, 1;
    %set/v v0061E138_0, 0, 1;
    %end;
    .scope S_00612C80;
t_0 %join;
    %end;
    .thread T_0;
    .scope S_00612C80;
T_1 ;
    %vpi_call 2 32 "$display", "Exemplo0004 - Rodolfo Herman - 451612";
    %vpi_call 2 33 "$display", "Test OR gate";
    %vpi_call 2 34 "$display", "\012a | b = s\012";
    %set/v v0061E0E0_0, 0, 1;
    %set/v v0061E138_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 36 "$display", "%b | %b = %b", v0061E0E0_0, v0061E138_0, v0061E190_0;
    %set/v v0061E0E0_0, 0, 1;
    %set/v v0061E138_0, 1, 1;
    %delay 1, 0;
    %vpi_call 2 38 "$display", "%b | %b = %b", v0061E0E0_0, v0061E138_0, v0061E190_0;
    %set/v v0061E0E0_0, 1, 1;
    %set/v v0061E138_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 40 "$display", "%b | %b = %b", v0061E0E0_0, v0061E138_0, v0061E190_0;
    %set/v v0061E0E0_0, 1, 1;
    %set/v v0061E138_0, 1, 1;
    %delay 1, 0;
    %vpi_call 2 42 "$display", "%b | %b = %b", v0061E0E0_0, v0061E138_0, v0061E190_0;
    %end;
    .thread T_1;
