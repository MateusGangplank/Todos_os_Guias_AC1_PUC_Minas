#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_002C2C80 .scope module, "testbuffer" "testbuffer" 2 18;
 .timescale 0 0;
v002CDC28_0 .var "a", 0 0;
v002CDC80_0 .net "s", 0 0, L_002CE180; 1 drivers
S_002C30C8 .scope module, "BF1" "buffer" 2 25, 2 11, S_002C2C80;
 .timescale 0 0;
L_002CE180 .functor BUFZ 1, v002CDC28_0, C4<0>, C4<0>, C4<0>;
v002C2D90_0 .net "p", 0 0, v002CDC28_0; 1 drivers
v002C3150_0 .alias "s", 0 0, v002CDC80_0;
S_002C3040 .scope begin, "start" "start" 2 27, 2 27, S_002C2C80;
 .timescale 0 0;
S_002C2D08 .scope begin, "main" "main" 2 31, 2 31, S_002C2C80;
 .timescale 0 0;
    .scope S_002C2C80;
T_0 ;
    %fork t_1, S_002C3040;
    %jmp t_0;
    .scope S_002C3040;
t_1 ;
    %set/v v002CDC28_0, 0, 1;
    %end;
    .scope S_002C2C80;
t_0 %join;
    %end;
    .thread T_0;
    .scope S_002C2C80;
T_1 ;
    %fork t_3, S_002C2D08;
    %jmp t_2;
    .scope S_002C2D08;
t_3 ;
    %vpi_call 2 33 "$display", "Exemplo 0001 - Rodolfo Herman - 451612";
    %vpi_call 2 34 "$display", "Test buffer:";
    %vpi_call 2 35 "$display", "\011 time\011a = s";
    %vpi_call 2 37 "$monitor", "%d\011%b = %b", $time, v002CDC28_0, v002CDC80_0;
    %delay 1, 0;
    %set/v v002CDC28_0, 1, 1;
    %delay 2, 0;
    %set/v v002CDC28_0, 0, 1;
    %end;
    .scope S_002C2C80;
t_2 %join;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Exemplo001.v";
