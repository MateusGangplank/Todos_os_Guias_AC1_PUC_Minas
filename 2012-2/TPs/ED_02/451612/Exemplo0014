#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0034CE80 .scope module, "test_base_01" "test_base_01" 2 10;
 .timescale 0 0;
v00349420_0 .var "a", 2 0;
v0034BED8_0 .var "b", 3 0;
v0034BF30_0 .var "c", 4 0;
v0034BF88_0 .var "d", 4 0;
    .scope S_0034CE80;
T_0 ;
    %vpi_call 2 18 "$display", "Exemplo0014 - Rodolfo Herman - 451612";
    %vpi_call 2 19 "$display", "Test number system";
    %movi 8, 5, 3;
    %set/v v00349420_0, 8, 3;
    %movi 11, 10, 4;
    %set/v v0034BED8_0, 11, 4;
    %movi 15, 15, 5;
    %set/v v0034BF30_0, 15, 5;
    %movi 15, 20, 5;
    %set/v v0034BF88_0, 15, 5;
    %vpi_call 2 24 "$display", "\012Positive value";
    %vpi_call 2 25 "$display", "a = %d = %3b", v00349420_0, v00349420_0;
    %vpi_call 2 26 "$display", "b = %d = %4b", v00349420_0, v00349420_0;
    %vpi_call 2 27 "$display", "c = %d = %5b", v00349420_0, v00349420_0;
    %vpi_call 2 28 "$display", "b = %d = %4b", v0034BED8_0, v0034BED8_0;
    %vpi_call 2 29 "$display", "c = %d = %5b", v0034BF30_0, v0034BF30_0;
    %vpi_call 2 30 "$display", "d = %d = %5b", v0034BF88_0, v0034BF88_0;
    %vpi_call 2 31 "$display", "d = %d = %5o", v0034BF88_0, v0034BF88_0;
    %vpi_call 2 32 "$display", "d = %d = %5h", v0034BF88_0, v0034BF88_0;
    %movi 8, 3, 3;
    %set/v v00349420_0, 8, 3;
    %movi 8, 11, 4;
    %set/v v0034BED8_0, 8, 4;
    %movi 12, 27, 5;
    %set/v v0034BF30_0, 12, 5;
    %vpi_call 2 37 "$display", "\012Negative value";
    %vpi_call 2 38 "$display", "a = -5 [3] = %d = %3b", v00349420_0, v00349420_0;
    %vpi_call 2 39 "$display", "b = -5 [4] = %d = %4b", v0034BED8_0, v0034BED8_0;
    %vpi_call 2 40 "$display", "c = -5 [5] = %d = %5b", v0034BF30_0, v0034BF30_0;
    %movi 8, 11, 4;
    %set/v v00349420_0, 8, 3;
    %movi 12, 11, 4;
    %set/v v0034BED8_0, 12, 4;
    %movi 16, 27, 5;
    %set/v v0034BF30_0, 16, 5;
    %vpi_call 2 45 "$display", "\012Negative mixed expression";
    %vpi_call 2 46 "$display", "a = %d = %3b", v00349420_0, v00349420_0;
    %vpi_call 2 47 "$display", "b = %d = %4b", v0034BED8_0, v0034BED8_0;
    %vpi_call 2 48 "$display", "c = %d = %5b", v0034BF30_0, v0034BF30_0;
    %movi 8, 8, 5;
    %set/v v00349420_0, 8, 3;
    %movi 8, 8, 5;
    %set/v v0034BED8_0, 8, 4;
    %movi 8, 34, 7;
    %set/v v0034BF30_0, 8, 5;
    %vpi_call 2 53 "$display", "\012Overflow";
    %vpi_call 2 54 "$display", "a = %d = %3b", v00349420_0, v00349420_0;
    %vpi_call 2 55 "$display", "b = %d = %4b", v0034BED8_0, v0034BED8_0;
    %vpi_call 2 56 "$display", "c = %d = %5b", v0034BF30_0, v0034BF30_0;
    %end;
    .thread T_0;
