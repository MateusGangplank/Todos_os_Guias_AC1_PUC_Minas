// Programa para calcular uma expressão do tipo
// dado04 = dado01 + dado02 - dado03
//
// dado04(resultado) sera salvo na posicao de memoria C053
// dado01 devera ser inserido manualmente na posicao de memoria C050
// dado02 devera ser inserido manualmente na posicao de memoria C051
// dado03 devera ser inserido manualmente na posicao de memoria C052

LDA C050;
MVI H,C0;
MVI L,51;
ADD M;
MVI L,52;
SUB M;
STA C053;
HLT;