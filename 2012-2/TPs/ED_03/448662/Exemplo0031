#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0038CFB8 .scope module, "test_f4" "test_f4" 2 32;
 .timescale 0 0;
v0038CA18_0 .var "chave", 0 0;
v0038CA70_0 .var "x", 3 0;
v0038CAC8_0 .var "y", 3 0;
v0038CB20_0 .net "z", 3 0, v0038A750_0; 1 drivers
S_00382D60 .scope module, "modulo" "f4" 2 41, 2 11, S_0038CFB8;
 .timescale 0 0;
v0038A648_0 .net "a", 3 0, v0038CA70_0; 1 drivers
v0038A6A0_0 .net "b", 3 0, v0038CAC8_0; 1 drivers
v0038A6F8_0 .net "chave", 0 0, v0038CA18_0; 1 drivers
v0038A750_0 .var "r", 3 0;
v0038A7A8_0 .alias "s", 3 0, v0038CB20_0;
E_0038E1B0 .event edge, v0038A6F8_0, v0038A6A0_0, v0038A648_0;
S_00382CD8 .scope begin, "start" "start" 2 44, 2 44, S_0038CFB8;
 .timescale 0 0;
S_0038D040 .scope begin, "main" "main" 2 50, 2 50, S_0038CFB8;
 .timescale 0 0;
    .scope S_00382D60;
T_0 ;
    %wait E_0038E1B0;
    %load/v 8, v0038A6F8_0, 1;
    %cmpi/u 8, 0, 1;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 1;
    %jmp/1 T_0.1, 6;
    %jmp T_0.3;
T_0.0 ;
    %load/v 8, v0038A648_0, 4;
    %load/v 12, v0038A6A0_0, 4;
    %and 8, 12, 4;
    %set/v v0038A750_0, 8, 4;
    %jmp T_0.3;
T_0.1 ;
    %load/v 8, v0038A648_0, 4;
    %load/v 12, v0038A6A0_0, 4;
    %or 8, 12, 4;
    %set/v v0038A750_0, 8, 4;
    %jmp T_0.3;
T_0.3 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0038CFB8;
T_1 ;
    %fork t_1, S_00382CD8;
    %jmp t_0;
    .scope S_00382CD8;
t_1 ;
    %movi 8, 3, 4;
    %set/v v0038CA70_0, 8, 4;
    %movi 8, 5, 4;
    %set/v v0038CAC8_0, 8, 4;
    %set/v v0038CA18_0, 0, 1;
    %end;
    .scope S_0038CFB8;
t_0 %join;
    %end;
    .thread T_1;
    .scope S_0038CFB8;
T_2 ;
    %fork t_3, S_0038D040;
    %jmp t_2;
    .scope S_0038D040;
t_3 ;
    %vpi_call 2 51 "$display", "Exemplo0031 - Josemar Alves Caetano - 448662";
    %vpi_call 2 52 "$display", "Test LU's module";
    %vpi_call 2 56 "$display", "\012Teste da opera\347\343o AND:\012";
    %delay 1, 0;
    %vpi_call 2 58 "$display", "%3b & %3b = %3b", v0038CA70_0, v0038CAC8_0, v0038CB20_0;
    %movi 8, 1, 4;
    %set/v v0038CA70_0, 8, 4;
    %movi 8, 4, 4;
    %set/v v0038CAC8_0, 8, 4;
    %delay 1, 0;
    %vpi_call 2 61 "$display", "%3b & %3b = %3b", v0038CA70_0, v0038CAC8_0, v0038CB20_0;
    %movi 8, 5, 4;
    %set/v v0038CA70_0, 8, 4;
    %movi 8, 4, 4;
    %set/v v0038CAC8_0, 8, 4;
    %delay 1, 0;
    %vpi_call 2 64 "$display", "%3b & %3b = %3b", v0038CA70_0, v0038CAC8_0, v0038CB20_0;
    %vpi_call 2 66 "$display", "\012Teste da opera\347\343o OR:\012";
    %delay 1, 0;
    %movi 8, 1, 4;
    %set/v v0038CA70_0, 8, 4;
    %movi 8, 4, 4;
    %set/v v0038CAC8_0, 8, 4;
    %set/v v0038CA18_0, 1, 1;
    %vpi_call 2 70 "$display", "%3b | %3b = %3b", v0038CA70_0, v0038CAC8_0, v0038CB20_0;
    %end;
    .scope S_0038CFB8;
t_2 %join;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
