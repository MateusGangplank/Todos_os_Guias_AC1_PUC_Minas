	load  	 dado01
	multiply valor1
	store    result1
	load  	 dado02
Loop:	subtract valor2
	jmpn     Done
	jump	 Loop
Done:   add      valor2
	add	 result1
	store    dado03
	stop	
dado01:	 .data 2 10 ; Primeiro dado
valor1:  .data 2 2  ; Fator multiplicativo
result1: .data 2 0  ; Resultado parcial
dado02:	 .data 2 41 ; Segundo dado
valor2:  .data 2 2  ; Valor do modulo
dado03:	 .data 2 0  ; Resultado da equacao dos dados