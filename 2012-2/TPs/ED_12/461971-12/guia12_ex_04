    %ix/load 1, 0, 0;
    %assign/v0/x1 v006E3098_0, 0, 8;
    %delay 1, 0;
    %load/v 8, v00766F80_0, 16;
    %load/v 24, v006E3098_0, 16;
    %and 8, 24, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v00766F80_0, 0, 8;
    %vpi_call 2 240 "$display", " PC=%d -> AC & [%h] = %h\012", v0074A718_0, v006E3098_0, v00766F80_0;
    %jmp T_2.18;
T_2.13 ;
    %delay 1, 0;
    %ix/getv 3, v006E3040_0;
    %load/av 8, v0074A610, 8;
    %ix/load 0, 8, 0;
    %ix/load 1, 8, 0;
    %assign/v0/x1 v006E3098_0, 0, 8;
    %ix/load 0, 1, 0;
    %load/vp0 16, v006E3040_0, 32;
    %ix/get 3, 16, 32;
    %load/av 8, v0074A610, 8;
    %ix/load 0, 8, 0;
    %ix/load 1, 0, 0;
    %assign/v0/x1 v006E3098_0, 0, 8;
    %delay 1, 0;
    %load/v 8, v00766F80_0, 16;
    %load/v 24, v006E3098_0, 16;
    %xor 8, 24, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v00766F80_0, 0, 8;
    %vpi_call 2 245 "$display", " PC=%d -> AC ^ [%h] = %h\012", v0074A718_0, v006E3098_0, v00766F80_0;
    %jmp T_2.18;
T_2.14 ;
    %delay 1, 0;
    %vpi_call 2 249 "$display", " PC=%d -> jump %d\012", v0074A718_0, v006E3040_0;
    %load/v 8, v006E3040_0, 10;
    %ix/load 0, 10, 0;
    %assign/v0 v0074A718_0, 0, 8;
    %jmp T_2.18;
T_2.15 ;
    %delay 1, 0;
    %vpi_call 2 254 "$display", " PC=%d -> jumpS (%h<0) %h\012", v0074A718_0, v00766F80_0, v006E3040_0;
    %ix/load 1, 15, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.21, 4;
    %load/x1p 8, v00766F80_0, 1;
    %jmp T_2.22;
T_2.21 ;
    %mov 8, 2, 1;
T_2.22 ;
; Save base=8 wid=1 in lookaside.
    %cmpi/u 8, 0, 1;
    %jmp/0xz  T_2.23, 4;
    %load/v 8, v006E3040_0, 10;
    %ix/load 0, 10, 0;
    %assign/v0 v0074A718_0, 0, 8;
T_2.23 ;
    %jmp T_2.18;
T_2.16 ;
    %delay 1, 0;
    %vpi_call 2 260 "$display", " PC=%d -> jumpZ (%h==0) %h\012", v0074A718_0, v00766F80_0, v006E3040_0;
    %load/v 8, v00766F80_0, 16;
    %cmpi/u 8, 0, 16;
    %jmp/0xz  T_2.25, 4;
    %load/v 8, v006E3040_0, 10;
    %ix/load 0, 10, 0;
    %assign/v0 v0074A718_0, 0, 8;
T_2.25 ;
    %jmp T_2.18;
T_2.17 ;
    %delay 1, 0;
    %set/v v0074A718_0, 0, 10;
    %vpi_call 2 266 "$display", " PC=%d -> halt\012", v0074A718_0;
    %jmp T_2.18;
T_2.18 ;
    %end;
    .scope S_00748510;
t_4 %join;
