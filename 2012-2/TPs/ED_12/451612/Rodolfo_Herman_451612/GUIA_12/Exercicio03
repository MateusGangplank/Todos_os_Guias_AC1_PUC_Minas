    %delay 1, 0;
    %load/v 8, v00763820_0, 16;
    %load/v 24, v00762548_0, 16;
    %xor 8, 24, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v00763820_0, 0, 8;
    %vpi_call 2 249 "$display", " PC=%d -> AC ^ [%h] = %h\012", v00852CF0_0, v00762548_0, v00763820_0;
    %jmp T_2.18;
T_2.14 ;
    %delay 1, 0;
    %vpi_call 2 253 "$display", " PC=%d -> jump %d\012", v00852CF0_0, v007624F0_0;
    %load/v 8, v007624F0_0, 10;
    %ix/load 0, 10, 0;
    %assign/v0 v00852CF0_0, 0, 8;
    %jmp T_2.18;
T_2.15 ;
    %delay 1, 0;
    %vpi_call 2 258 "$display", " PC=%d -> jumpS (%h<0) %h\012", v00852CF0_0, v00763820_0, v007624F0_0;
