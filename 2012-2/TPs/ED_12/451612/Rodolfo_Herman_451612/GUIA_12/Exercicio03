    %mov 4, 0, 1;
    %jmp/1 T_2.21, 4;
    %load/x1p 8, v00763820_0, 1;
    %jmp T_2.22;
T_2.21 ;
    %mov 8, 2, 1;
T_2.22 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_2.23, 8;
    %load/v 8, v007624F0_0, 10;
    %ix/load 0, 10, 0;
    %assign/v0 v00852CF0_0, 0, 8;
T_2.23 ;
    %jmp T_2.18;
T_2.16 ;
    %delay 1, 0;
    %vpi_call 2 264 "$display", " PC=%d -> jumpZ (%h==0) %h\012", v00852CF0_0, v00763820_0, v007624F0_0;
    %load/v 8, v00763820_0, 16;
    %cmpi/u 8, 0, 16;
    %jmp/0xz  T_2.25, 4;
    %load/v 8, v007624F0_0, 10;
