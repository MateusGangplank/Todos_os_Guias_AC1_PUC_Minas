    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.3, 6;
    %set/v v00343048_0, 2, 2;
    %jmp T_0.5;
T_0.0 ;
    %load/v 8, v0034E2F0_0, 1;
    %jmp/0xz  T_0.6, 8;
    %movi 8, 1, 2;
    %set/v v00343048_0, 8, 2;
    %jmp T_0.7;
