#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_009848C8 .scope module, "Exemplo0042" "Exemplo0042" 2 49;
 .timescale 0 0;
v0098ACA0_0 .net "clock", 0 0, v0098AC48_0; 1 drivers
v0098ACF8_0 .var "p", 0 0;
v0098AD50_0 .net "p1", 0 0, v0098D950_0; 1 drivers
v0098ADA8_0 .net "t1", 0 0, v0098D8A0_0; 1 drivers
S_0031F898 .scope module, "clk" "clock" 2 56, 3 11, S_009848C8;
 .timescale 0 0;
v0098AC48_0 .var "clk", 0 0;
S_0031F810 .scope module, "pulse1" "pulse" 2 57, 2 14, S_009848C8;
 .timescale 0 0;
v0098D8F8_0 .alias "clock", 0 0, v0098ACA0_0;
v0098D950_0 .var "signal", 0 0;
E_0098CF28 .event edge, v00982B78_0;
S_009835C8 .scope module, "trigger1" "trigger" 2 58, 2 33, S_009848C8;
 .timescale 0 0;
L_0098E2E8 .functor AND 1, v0098AC48_0, v0098ACF8_0, C4<1>, C4<1>;
v00982B20_0 .net *"_s1", 0 0, L_0098E2E8; 1 drivers
v00982B78_0 .alias "clock", 0 0, v0098ACA0_0;
v00982BD0_0 .net "on", 0 0, v0098ACF8_0; 1 drivers
v0098D8A0_0 .var "signal", 0 0;
E_0098CF48 .event posedge, L_0098E2E8;
S_00983540 .scope begin, "main" "main" 2 60, 2 60, S_009848C8;
 .timescale 0 0;
    .scope S_0031F898;
T_0 ;
    %set/v v0098AC48_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0031F898;
T_1 ;
    %delay 12, 0;
    %load/v 8, v0098AC48_0, 1;
    %inv 8, 1;
    %set/v v0098AC48_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0031F810;
T_2 ;
    %wait E_0098CF28;
    %set/v v0098D950_0, 1, 1;
    %delay 3, 0;
    %set/v v0098D950_0, 0, 1;
    %delay 3, 0;
    %set/v v0098D950_0, 1, 1;
    %delay 3, 0;
    %set/v v0098D950_0, 0, 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_009835C8;
T_3 ;
    %wait E_0098CF48;
    %delay 60, 0;
    %set/v v0098D8A0_0, 1, 1;
    %delay 60, 0;
    %set/v v0098D8A0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_009848C8;
T_4 ;
    %fork t_1, S_00983540;
    %jmp t_0;
    .scope S_00983540;
t_1 ;
    %set/v v0098ACF8_0, 0, 1;
    %end;
    .scope S_009848C8;
t_0 %join;
    %end;
    .thread T_4;
    .scope S_009848C8;
T_5 ;
    %vpi_call 2 65 "$dumpfile", "Exemplo0042.vcd";
    %vpi_call 2 66 "$dumpvars", 2'sb01, v0098ACA0_0, v0098AD50_0, v0098ACF8_0, v0098ADA8_0;
    %delay 60, 0;
    %set/v v0098ACF8_0, 1, 1;
    %delay 120, 0;
    %set/v v0098ACF8_0, 0, 1;
    %delay 180, 0;
    %set/v v0098ACF8_0, 1, 1;
    %delay 240, 0;
    %set/v v0098ACF8_0, 0, 1;
    %delay 300, 0;
    %set/v v0098ACF8_0, 1, 1;
    %delay 360, 0;
    %set/v v0098ACF8_0, 0, 1;
    %delay 376, 0;
    %vpi_call 2 73 "$finish";
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
