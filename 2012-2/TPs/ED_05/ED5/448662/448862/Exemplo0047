#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_003E35C8 .scope module, "Exemplo0047" "Exemplo0047" 2 31;
 .timescale 0 0;
v003E43C0_0 .net "clock", 0 0, v003E4368_0; 1 drivers
v003EDFF0_0 .net "p", 0 0, v003E2C00_0; 1 drivers
S_003E42E0 .scope module, "clk" "clock" 2 38, 3 11, S_003E35C8;
 .timescale 0 0;
v003E4368_0 .var "clk", 0 0;
S_003E2B20 .scope module, "pls3" "pulse3" 2 39, 2 14, S_003E35C8;
 .timescale 0 0;
v003E2BA8_0 .alias "clock", 0 0, v003E43C0_0;
v003E2C00_0 .var "signal", 0 0;
E_002E0DF8 .event negedge, v003E2BA8_0;
    .scope S_003E42E0;
T_0 ;
    %set/v v003E4368_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_003E42E0;
T_1 ;
    %delay 12, 0;
    %load/v 8, v003E4368_0, 1;
    %inv 8, 1;
    %set/v v003E4368_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_003E2B20;
T_2 ;
    %wait E_002E0DF8;
    %set/v v003E2C00_0, 0, 1;
    %delay 5, 0;
    %set/v v003E2C00_0, 1, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_003E35C8;
T_3 ;
    %vpi_call 2 43 "$dumpfile", "Exemplo0047.vcd";
    %vpi_call 2 44 "$dumpvars", 2'sb01, v003E43C0_0, v003EDFF0_0;
    %delay 120, 0;
    %vpi_call 2 45 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Exemplo0047.v";
    "./clock.v";
