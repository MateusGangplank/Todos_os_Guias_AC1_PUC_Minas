#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00BBAB50 .scope module, "test_base_01" "test_base_01" 2 9;
 .timescale 0 0;
v00BBFF80_0 .var "a", 2 0;
v00BBD9E0_0 .var "b", 3 0;
v00BBF3B8_0 .var "c", 4 0;
v00BBDE48_0 .var "d", 4 0;
    .scope S_00BBAB50;
T_0 ;
    %vpi_call 2 17 "$display", "Exemplo0013 - Gustavo Barbosa - 427410";
    %vpi_call 2 18 "$display", "Test number system";
    %movi 8, 5, 3;
    %set/v v00BBFF80_0, 8, 3;
    %movi 11, 10, 4;
    %set/v v00BBD9E0_0, 11, 4;
    %movi 15, 15, 5;
    %set/v v00BBF3B8_0, 15, 5;
    %movi 15, 20, 5;
    %set/v v00BBDE48_0, 15, 5;
    %vpi_call 2 23 "$display", "\012Positive value";
    %vpi_call 2 24 "$display", "a = %d = %3b", v00BBFF80_0, v00BBFF80_0;
    %vpi_call 2 25 "$display", "b = %d = %4b", v00BBFF80_0, v00BBFF80_0;
    %vpi_call 2 26 "$display", "c = %d = %5b", v00BBFF80_0, v00BBFF80_0;
    %vpi_call 2 27 "$display", "b = %d = %4b", v00BBD9E0_0, v00BBD9E0_0;
    %vpi_call 2 28 "$display", "c = %d = %5b", v00BBF3B8_0, v00BBF3B8_0;
    %vpi_call 2 29 "$display", "d = %d = %5b", v00BBDE48_0, v00BBDE48_0;
    %vpi_call 2 30 "$display", "d = %d = %5o", v00BBDE48_0, v00BBDE48_0;
    %vpi_call 2 31 "$display", "d = %d = %5h", v00BBDE48_0, v00BBDE48_0;
    %movi 8, 3, 3;
    %set/v v00BBFF80_0, 8, 3;
    %movi 8, 11, 4;
    %set/v v00BBD9E0_0, 8, 4;
    %movi 12, 27, 5;
    %set/v v00BBF3B8_0, 12, 5;
    %vpi_call 2 35 "$display", "\012Negative value";
    %vpi_call 2 36 "$display", "a = -5 [3] = %d = %3b", v00BBFF80_0, v00BBFF80_0;
    %vpi_call 2 37 "$display", "b = -5 [4] = %d = %4b", v00BBD9E0_0, v00BBD9E0_0;
    %vpi_call 2 38 "$display", "c = -5 [5] = %d = %5b", v00BBF3B8_0, v00BBF3B8_0;
    %movi 8, 11, 4;
    %set/v v00BBFF80_0, 8, 3;
    %movi 12, 11, 4;
    %set/v v00BBD9E0_0, 12, 4;
    %movi 16, 27, 5;
    %set/v v00BBF3B8_0, 16, 5;
    %vpi_call 2 43 "$display", "\012Negative mixed expression";
    %vpi_call 2 44 "$display", "a = %d = %3b", v00BBFF80_0, v00BBFF80_0;
    %vpi_call 2 45 "$display", "b = %d = %4b", v00BBD9E0_0, v00BBD9E0_0;
    %vpi_call 2 46 "$display", "c = %d = %5b", v00BBF3B8_0, v00BBF3B8_0;
    %end;
    .thread T_0;
