#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_007BDCB0 .scope module, "operacoes" "operacoes" 2 6;
 .timescale 0 0;
v007BDD38_0 .var "a", 4 0;
v007BFEA8_0 .var "b", 4 0;
v007BFF00_0 .var "c", 3 0;
v007BFF58_0 .var "d", 3 0;
v007B2CF8_0 .var "e", 3 0;
    .scope S_007BDCB0;
T_0 ;
    %vpi_call 2 16 "$display", "Exercicio1 - Gustavo Barbosa - 427410";
    %vpi_call 2 17 "$display", "Operacoes resultando em binario ";
    %movi 8, 17, 6;
    %set/v v007BDD38_0, 8, 5;
    %vpi_call 2 20 "$display", "\012a = 2+15";
    %vpi_call 2 21 "$display", "a = %d = %5b", v007BDD38_0, v007BDD38_0;
    %movi 8, 24, 6;
    %set/v v007BFEA8_0, 8, 5;
    %vpi_call 2 24 "$display", "\012b = 3*8";
    %vpi_call 2 25 "$display", "b = %d = %5b", v007BFEA8_0, v007BFEA8_0;
    %movi 8, 10, 5;
    %set/v v007BFF00_0, 8, 4;
    %vpi_call 2 28 "$display", "\012c = 32/3";
    %vpi_call 2 29 "$display", "c = %d = %4b", v007BFF00_0, v007BFF00_0;
    %movi 8, 10, 5;
    %set/v v007BFF58_0, 8, 4;
    %vpi_call 2 32 "$display", "\012d = 21-11";
    %vpi_call 2 33 "$display", "d = %d = %4b", v007BFF58_0, v007BFF58_0;
    %movi 8, 15, 5;
    %set/v v007B2CF8_0, 8, 4;
    %vpi_call 2 36 "$display", "\012e = 2*5+6-1";
    %vpi_call 2 37 "$display", "e = %d = %4b", v007B2CF8_0, v007B2CF8_0;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Exercicio1.v";
