S_0082D498 .scope module, "operacoes" "operacoes" 2 6;
 .timescale 0 0;
v0082E1C0_0 .var "a", 6 0;
v0082E300_0 .var "b", 5 0;
v00824C88_0 .var "c", 3 0;
v0082D520_0 .var "d", 6 0;
v00822D10_0 .var "e", 9 0;
    .scope S_0082D498;
T_0 ;
    %vpi_call 2 16 "$display", "Exercicio2 - Gustavo Barbosa - 427410";
    %vpi_call 2 17 "$display", "Operacoes resultando em binario ";
    %movi 8, 68, 7;
    %set/v v0082E1C0_0, 8, 7;
    %vpi_call 2 20 "$display", "\012a = %d = %7b", v0082E1C0_0, v0082E1C0_0;
    %movi 8, 47, 6;
    %set/v v0082E300_0, 8, 6;
    %vpi_call 2 23 "$display", "\012b = %d = %7b", v0082E300_0, v0082E300_0;
    %movi 8, 15, 10;
    %set/v v00824C88_0, 8, 4;
    %vpi_call 2 26 "$display", "\012c = %d = %7b", v00824C88_0, v00824C88_0;
    %movi 8, 89, 9;
    %set/v v0082D520_0, 8, 7;
    %vpi_call 2 29 "$display", "\012d = %d = %6b", v0082D520_0, v0082D520_0;
    %movi 8, 647, 20;
    %set/v v00822D10_0, 8, 10;
    %vpi_call 2 32 "$display", "\012e = %d = %6b", v00822D10_0, v00822D10_0;
    %end;
    .thread T_0;
