#! /c/iverilog/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_005EE888 .scope module, "testmodule" "testmodule" 2 24;
 .timescale 0 0;
v005E37F8_0 .net "clock", 0 0, v005E37A0_0; 1 drivers
v005E2CF8_0 .net "p1", 0 0, v005E4C20_0; 1 drivers
S_005E3718 .scope module, "clk" "clock" 2 26, 3 4, S_005EE888;
 .timescale 0 0;
v005E37A0_0 .var "clk", 0 0;
S_005E4B40 .scope module, "pls" "pulse" 2 29, 2 8, S_005EE888;
 .timescale 0 0;
v005E4BC8_0 .alias "clock", 0 0, v005E37F8_0;
v005E4C20_0 .var "signal", 0 0;
E_005ED038 .event posedge, v005E4BC8_0;
    .scope S_005E3718;
T_0 ;
    %set/v v005E37A0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_005E3718;
T_1 ;
    %delay 12, 0;
    %load/v 8, v005E37A0_0, 1;
    %inv 8, 1;
    %set/v v005E37A0_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_005E4B40;
T_2 ;
    %wait E_005ED038;
    %set/v v005E4C20_0, 1, 1;
    %delay 4, 0;
    %set/v v005E4C20_0, 0, 1;
    %delay 4, 0;
    %set/v v005E4C20_0, 1, 1;
    %delay 4, 0;
    %set/v v005E4C20_0, 0, 1;
    %delay 4, 0;
    %set/v v005E4C20_0, 1, 1;
    %delay 4, 0;
    %set/v v005E4C20_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_005EE888;
T_3 ;
    %vpi_call 2 32 "$display", "Exemplo0045 - Gustavo Barbosa - 427410";
    %vpi_call 2 33 "$dumpfile", "Exemplo0045.vcd";
    %vpi_call 2 34 "$dumpvars";
    %delay 276, 0;
    %vpi_call 2 35 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Exemplo0045.v";
    "./clock.v";
