module f4 (output [5:0] s, 
input [5:0] a, 
input [5:0] b);
assign s1 = (a | b);
endmodule // f4 

 


// ------------------------- definir dados 
reg [5:0] x; 
reg [5:0] y; 
wire [5:0] z; 

f4 modulo (z, x, y); 

 


